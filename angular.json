{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@ng-ar/ar-popover": {
      "projectType": "library",
      "root": "projects/ng-ar/ar-popover",
      "sourceRoot": "projects/ng-ar/ar-popover/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ng-ar/ar-popover/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-ar/ar-popover/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ng-ar/ar-popover/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ng-ar/ar-popover/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "popper-test2": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/popper-test2",
      "sourceRoot": "projects/popper-test2/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/popper-test2",
            "index": "projects/popper-test2/src/index.html",
            "main": "projects/popper-test2/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/popper-test2/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/popper-test2/src/favicon.ico",
              "projects/popper-test2/src/assets"
            ],
            "styles": [
              "projects/popper-test2/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "popper-test2:build:production"
            },
            "development": {
              "buildTarget": "popper-test2:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "popper-test2:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/popper-test2/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/popper-test2/src/favicon.ico",
              "projects/popper-test2/src/assets"
            ],
            "styles": [
              "projects/popper-test2/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "deploy": {
          "builder": "angular-cli-ghpages:deploy"
        }
      }
    }
  },
  "cli": {
    "analytics": "5b896f63-a564-46b5-832a-4660d1844a25"
  }
}
